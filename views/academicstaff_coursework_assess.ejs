<% include partials/academicstaff_header %>
<div class="container">
   <h1>Assess coursework</h1>

    <table class="table table-striped table-hover table-staff table-bordered">
      <thead>
         <tr>
            <th>Module Code</th>
            <th>File</th>
         </tr>
      </thead>
      <tbody>
         <% data.forEach(function(row){ %>
         <tr>
            <td> <%= row.ModuleCode %> </td>
            <td> <%= row.FileName %> <button type="button">Click Me!</button></td> 
         </tr>
         <% });%>
      </tbody>
   </table>

</div>
<% include partials/footer %>